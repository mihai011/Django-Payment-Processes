    "version": "0.2.0",
    "configurations": [

        
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/DjangoPaymentProcesses/manage.py",
            "args": [
                "runserver",
                "--noreload"
            ],
            "django": true
        },

        {
            "name": "Python: TestDjangoPayments",
            "type": "python",
            "request": "launch",
            "program":"${workspaceFolder}/DjangoPaymentProcesses/manage.py",
            "args": [
                "test",
                "DjangoPaymentProcesses/payments",
                "--noinput",
                "--keepdb"
            ],
            "django": true
        },

        {
            "name": "Python: TestDjangoPrediction",
            "type": "python",
            "request": "launch",
            "program":"${workspaceFolder}/DjangoPaymentProcesses/manage.py",
            "args": [
                "test",
                "DjangoPaymentProcesses/prediction",
                "--noinput",
                "--keepdb"
            ],
            "django": true
        }
    ]
}